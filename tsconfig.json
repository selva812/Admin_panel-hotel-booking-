{
  "compilerOptions": {
    "target": "ESNext",
    "downlevelIteration": true,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@core/*": [
        "./src/@core/*"
      ],
      "@layouts/*": [
        "./src/@layouts/*"
      ],
      "@menu/*": [
        "./src/@menu/*"
      ],
      "@assets/*": [
        "./src/assets/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@configs/*": [
        "./src/configs/*"
      ],
      "@views/*": [
        "./src/views/*"
      ]
    }
  },
  "include": [
    "next.config.ts",
    "tailwind.config.ts",
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/libs/auth.ts",
    "src/app/(dashboard)/(private)/dashboards/booking-calender/page.js",
    "src/app/(dashboard)/(private)/dashboards/booking/details/page.js",
    "src/app/(dashboard)/(private)/dashboards/service/page.js",
    "src/app/(dashboard)/(private)/dashboards/invoices/print/page.js",
    "src/app/(dashboard)/(private)/dashboards/checkout/page.js",
    "src/app/(dashboard)/(private)/dashboards/checkin/page.tsx",
    "src/components/dashboard/check-in/page.js",
    "src/components/dashboard/check-in/occupancyform.js",
    "src/components/dashboard/check-in/bookingdetail.js",
    "src/components/dashboard/booking/page.tsx",
    "src/components/dailybooking.js",
    "src/app/nprogress.tsx",
    "src/app/api/upload/route.js",
    "src/components/dashboard/check-in/checkinform.js",
    "src/components/mobilecalender.js",
    "src/app/(dashboard)/(private)/dashboards/checkout/details/page.js",
    "src/app/(dashboard)/(private)/dashboards/customers/details/page.js",
    "src/app/(dashboard)/(private)/dashboards/settings/page.js",
    "src/components/layout/vertical/user.js",
    "src/app/(dashboard)/(private)/dashboards/report/page.tsx",
    "src/components/advancebookingmodal.js",
    "src/app/(dashboard)/(private)/dashboards/booking/create/page.js"
  ],
  "exclude": [
    "node_modules"
  ]
}
